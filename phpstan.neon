parameters:
    level: max
    paths:
        - src/
        - infrastructures/
        - definitions/
    ignoreErrors:
        -
            message: '#will always evaluate to true#'
            path: src/Client/Client.php
        - '#Property [a-zA-Z0-9\\_]+::\$apiUrl \(array<string, string>\) does not accept array<string, int\|string>\|false.#'
        - '#mixed#'
        - '#^Offset#'
